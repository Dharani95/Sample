<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"  xmlns:t="sap.ui.table" xmlns:html="http://www.w3.org/1999/xhtml">
	 <Dialog id='PMsubmission' title="Unallocated % against the requested RLS" >
		
					<t:Table id="idPlanResourceTbl"  class="pMTableClass" fixedColumnCount="3" enableColumnReordering="false"
						rows="{oResponseRlsData>/}" 
						selectionMode="None"
						group="subGrade"
						sort="subGrade"
						visibleRowCount="8" >
						<t:toolbar>
							<Toolbar>
								<Title  level="H3" titleStyle="H3" />
								<ToolbarSpacer />
								<Button text="Submit" press="onFragSubmitButton" type="Emphasized" class="sapUiSizeCompact" />
								<Button text="Cancel" press="onFragCancelButton" type="Reject" class="sapUiSizeCompact" />
							</Toolbar>
						</t:toolbar>
					</t:Table>
		
	</Dialog> 
	<!-- <ResponsivePopover id='PMsubmission' 
		placement="Bottom" 
		title="User Details"
		contentWidth='22rem' contentHeight='10rem'>
	</ResponsivePopover> -->

</core:FragmentDefinition>
